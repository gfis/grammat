/.CAPTEST LOGON
/.CAPTEST DO DO.EXTRA,('(PRIMARY)',PARM='ASSERT=200')
EOF IDENTIFIER NUMBER STRING
[AXIOM = KOMMANDOFOLGE
.KOMMANDOFOLGE = BEFEHL
               | KOMMANDOFOLGE ';' BEFEHL
.BEFEHL = BREAK_BEFEHL
         | CLOCK_BEFEHL
         | DISPLAY_BEFEHL
         | EQUATE BEZEICHNUNG ADRESSAUSDRUCK
         | EXECUTE DATEINAME
         | GO
         | HELP_BEFEHL
         | IMPLIZIT ADRESSAUSDRUCK IMPL_LAENGE
         | PRINT_BEFEHL
         | SET_BEFEHL
         | STOP
         | TRACE_BEFEHL
.BREAK_BEFEHL = BREAK ADRESSAUSDRUCK BREAK_MODUS
.BREAK_MODUS = ALTER
             | REFER
             | ON
             | OFF
.CLOCK_BEFEHL = CLOCK CLOCK_MODUS
.CLOCK_MODUS = ON BEZEICHNUNG
             | OFF BEZEICHNUNG
             | OFF
             | DISPLAY BEZEICHNUNG
             | DISPLAY
.DISPLAY_BEFEHL = DISPLAY DISPLAY_OBJEKT
.DISPLAY_OBJEKT = OBJEKT_MIT_TYP
                | DISPLAY_OBJEKT ',' OBJEKT_MIT_TYP
.OBJEKT_MIT_TYP = ADRESSAUSDRUCK
                | ADRESSAUSDRUCK '/' LAENGE_TYP
.LAENGE_TYP = LAENGE
             | TYP
            | LAENGE TYP
.LAENGE = DEZIMALZAHL
.IMPL_LAENGE =
             | LAENGE_TYP
.TYP = R2
     | R8
     | R10
     | R16
     | CHAR
.HELP_BEFEHL = HELP H_KOMMANDO HELP_TEILE
.H_KOMMANDO = BREAK
            | CLOCK
            | DISPLAY
            | EQUATE
            | EXECUTE
            | GO
            | HELP
            | IMPLIZIT
            | PRINT
            | SET
            | STOP
            | TRACE
.HELP_TEILE =
          | HELP_TEILE TEIL
.TEIL = SYNTAX
      | FUNKTION
      | EXAMPLE
.PRINT_BEFEHL = PRINT DATEINAME KONSTANTE
.KONSTANTE =
           | DEZIMALZAHL
           | DEZIMALZAHL DEZIMALZAHL
.SET_BEFEHL = SET SET_OBJEKT
.SET_OBJEKT = ADRESSAUSDRUCK SET_LAENGE '=' WERTE
            | SET_OBJEKT ',' ADRESSAUSDRUCK SET_LAENGE '=' WERTE
.SET_LAENGE =
            | '/' DEZIMALZAHL
.WERTE = S_ANZAHL S_WERT
       | WERTE S_ANZAHL S_WERT
.S_ANZAHL =
          | DEZIMALZAHL '*'
.S_WERT = ZAHLKONSTANTE
       | ZEICHENREIHE
.TRACE_BEFEHL = TRACE ON TRACE_ANZAHL
              | TRACE OFF TRACE_ANZAHL
.TRACE_ANZAHL =
              | ANZAHL
.]
/LOGOFF
