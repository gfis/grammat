/.SPM LOGON
/DO DO.EXTRA,('(PRIMARY)',PARM='ASSERT=200')
EOF BEZEICHNER GANZZAHL STRINGKONSTANTE
[AXIOM = PROGRAMM
.PROGRAMM = ANWEISUNGSLISTE EOF
.ANWEISUNGSLISTE = Y1ANW
                 | ANWEISUNGSLISTE ';' Y1ANW
.Y1ANW = VEREINBARUNGSANWEISUNG
       | PROZEDURANWEISUNG
.PROZEDURANWEISUNG = PROC BEZEICHNER Y2RUMPF
.Y2RUMPF = EXT
         | Y3SPEZLI IS ANWEISUNG
.Y3SPEZLI =
          | '(' SPEZ_LISTE ')'
.SPEZ_LISTE = SPEZIFIKATION
            | SPEZ_LISTE ',' SPEZIFIKATION
.SPEZIFIKATION = BEZEICHNER S_ATTRIBUT
               | SPEZIFIKATION S_ATTRIBUT
.S_ATTRIBUT = TYPATTRIBUT
            | STRUKTURATTRIBUT
.VEREINBARUNGSANWEISUNG = DCL_ANWEISUNG
                        | FILE_ANWEISUNG
.DCL_ANWEISUNG = DCL DATENVEREINBARUNG
               | DCL_ANWEISUNG ',' DATENVEREINBARUNG
.DATENVEREINBARUNG = BEZEICHNER DATENATTRIBUT
.DATENATTRIBUT = TYPATTRIBUT
               | ORTSATTRIBUT
               | STRUKTURATTRIBUT
               | WERTATTRIBUT
.TYPATTRIBUT = BIN '(' GANZZAHL ')'
             | DEC '(' GANZZAHL ')'
             | STR '(' GANZZAHL ')'
             | PRT
.ORTSATTRIBUT = REG '(' GANZZAHL ')'
.STRUKTURATTRIBUT = COM '(' DATENVEREINBARUNG ')'
                  | OCC '(' GANZZAHL ')'
                  | RDF '(' BEZEICHNER ')'
.WERTATTRIBUT = VAL '(' KONSTANTENLISTE ')'
.FILE_ANWEISUNG = FILE DATEIVEREINBARUNG
.DATEIVEREINBARUNG = BEZEICHNER DATEITYPATTRIBUT
   ASSIGN '(' HEXADEZIMALZAHL ')'
   RECORD '(' BEZEICHNER ')'
   Y4BLOCK
   Y5KEY
   STATUS '(' BEZEICHNER ')'
.Y4BLOCK =
         | BLOCK '(' GANZZAHL ')'
.Y5KEY =
       | KEY '(' BEZEICHNER ')'
.DATEITYPATTRIBUT = INDEX
                  | RAND
                  | SEQU
.ANWEISUNG = Y6BEZ VEREINBARUNGSANWEISUNG
           | Y6BEZ PROZEDURANWEISUNG
           | Y6BEZ PROGRAMMANWEISUNG
.Y6BEZ =
       | BEZEICHNER ':'
.PROGRAMMANWEISUNG = STRUKTURANWEISUNG
                   | AKTIONSANWEISUNG
                   | DATEIANWEISUNG
                   | SYSTEMANWEISUNG
.STRUKTURANWEISUNG = BEG_ANWEISUNG
                   | CALL_ANWEISUNG
                   | CASE_ANWEISUNG
                   | FOR_ANWEISUNG
                   | GOTO_ANWEISUNG
                   | IF_ANWEISUNG
                   | REPEAT_ANWEISUNG
                   | WHILE_ANWEISUNG
.AKTIONSANWEISUNG = ACC_ANWEISUNG
                  | LET_ANWEISUNG
.DATEIANWEISUNG = CLOSE_ANWEISUNG
                | DELETE_ANWEISUNG
                | OPEN_ANWEISUNG
                | READ_ANWEISUNG
                | UPDATE_ANWEISUNG
                | WRITE_ANWEISUNG
                | TEST_ANWEISUNG
.SYSTEMANWEISUNG = CODE_ANWEISUNG
                  | SVC_ANWEISUNG
.BEG_ANWEISUNG = BEG Y7ANW END
.Y7ANW = ANWEISUNG
       | Y7ANW ';' ANWEISUNG
.CALL_ANWEISUNG = CALL BEZEICHNER
                | CALL BEZEICHNER '(' PARAMETERLISTE ')'
.PARAMETERLISTE = AUSDRUCK
                | PARAMETERLISTE ',' AUSDRUCK
.CASE_ANWEISUNG = CASE AUSDRUCK IN BEG_ANWEISUNG Y9ERR
.Y9ERR =
       | ERR ANWEISUNG
.FOR_ANWEISUNG = FOR ZUWEISUNG Y10STEP TO AUSDRUCK DO ANWEISUNG
.Y10STEP =
         | UP   AUSDRUCK
         | DOWN AUSDRUCK
.GOTO_ANWEISUNG = GOTO BEZEICHNER
.IF_ANWEISUNG =  IF ENTSCHEIDUNG THEN ANWEISUNG
              |  IF ENTSCHEIDUNG THEN ANWEISUNG ELSE ANWEISUNG
.REPEAT_ANWEISUNG = REPEAT ANWEISUNG UNTIL ENTSCHEIDUNG
.WHILE_ANWEISUNG = WHILE ENTSCHEIDUNG DO ANWEISUNG
.ACC_ANWEISUNG = ACC AKQUENZ
               | ACC_ANWEISUNG ',' AKQUENZ
.LET_ANWEISUNG = LET ZUWEISUNG
               | LET_ANWEISUNG ',' ZUWEISUNG
.CLOSE_ANWEISUNG = CLOSE BEZEICHNER
.DELETE_ANWEISUNG = DELETE BEZEICHNER Y12KEY
.Y12KEY =
        | KEY AUSDRUCK
.OPEN_ANWEISUNG = OPEN BEZEICHNER Y13TRANS
.Y13TRANS = INPUT
          | OUTPUT
          | INOUT
          | EXTEND
.READ_ANWEISUNG = READ BEZEICHNER Y12KEY
.UPDATE_ANWEISUNG = UPDATE BEZEICHNER Y12KEY
.WRITE_ANWEISUNG = WRITE BEZEICHNER Y12KEY
.TEST_ANWEISUNG = TEST BEZEICHNER ERR ANWEISUNG
.CODE_ANWEISUNG = CODE MC_BEFEHL
                | CODE_ANWEISUNG ',' MC_BEFEHL
.SVC_ANWEISUNG = SVC BEZEICHNER
.ENTSCHEIDUNG = ODER_TERM
.ODER_TERM = UND_TERM
           | ODER_TERM OR UND_TERM
.UND_TERM = BEDINGUNG
          | UND_TERM AND BEDINGUNG
.BEDINGUNG = Y15NOT '(' ODER_TERM ')'  /* ECKIGE KLAMMERN ? */
           | Y15NOT VERGLEICH
           | Y15NOT BEDINGUNGSANZEIGER
.Y15NOT =
        | NOT
.VERGLEICH = AUSDRUCK Y16REL AUSDRUCK
.Y16REL = '='
        | '<'
        | '>'
        | '/='
        | '<='
        | '>='
.BEDINGUNGSANZEIGER = OVFL
                    | CARRY
.ZUWEISUNG = VARIABLE '=' AUSDRUCK
.AKQUENZ = AUSDRUCK
 /* AB HIER ANDERS - MEHRDEUTIGKEIT FUER Y17SIGO */
.AUSDRUCK = SUMME
          | Y18SIG SUMME
.SUMME = PRODUKT
       | SUMME Y18SIG PRODUKT
 /* AB HIER WIEDER 1 ZU 1 */
.Y17SIGO =
         | '+'
         | '-'
.Y18SIG = '+'
        | '-'
.PRODUKT = ANTIVALENZ
         | PRODUKT Y19MULT ANTIVALENZ
.Y19MULT = '*'
         | '/'
.ANTIVALENZ = DISJUNKTION
            | ANTIVALENZ '?' DISJUNKTION
.DISJUNKTION = KONJUNKTION
             | DISJUNKTION '!' KONJUNKTION
.KONJUNKTION = LOG_OPERATION
             | KONJUNKTION '&' LOG_OPERATION
.LOG_OPERATION = SCHIEBUNG
               | ' ' SCHIEBUNG
.SCHIEBUNG = OPERAND
           | SCHIEBUNG Y20PFEIL OPERAND
.Y20PFEIL = '<-'
          | '->'
          | '<<'
          | '>>'
.OPERAND = VARIABLE
         | GANZZAHL
         | '#' BEZEICHNER
         | STRINGKONSTANTE
   /* STATT DER LETZTEN 3 STAND ' | KONSTANTE ' */
         | '(' AUSDRUCK ')'
.VARIABLE = BEZEICHNER Y21IND
          | ALPHA_BEZEICHNER
          | OMEGA_BEZEICHNER
.Y21IND =
        | '(' GANZZAHL ')'
        | '(' BEZEICHNER Y22ADD ')'
.Y22ADD =
        | Y18SIG GANZZAHL
.KONSTANTENLISTE = KONSTANTE
                 | ',' KONSTANTE
.KONSTANTE = DEZIMALKONSTANTE
           | '#' BEZEICHNER
           | STRINGKONSTANTE
.DEZIMALKONSTANTE = Y17SIGO GANZZAHL
.ALPHA_BEZEICHNER = '@' BEZEICHNER
.OMEGA_BEZEICHNER = '$' BEZEICHNER
.
]
/LOGOFF
